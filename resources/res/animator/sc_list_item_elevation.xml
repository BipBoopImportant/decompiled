<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto">
    <item android:state_selected="true">
        <objectAnimator android:duration="@android:integer/config_shortAnimTime" android:valueTo="16dp" android:valueType="floatType" android:propertyName="translationZ"/>
    </item>
    <item android:state_selected="false">
        <objectAnimator android:duration="@android:integer/config_shortAnimTime" android:valueTo="0dp" android:valueType="floatType" android:propertyName="translationZ"/>
    </item>
</selector>
