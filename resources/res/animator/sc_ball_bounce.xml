<?xml version="1.0" encoding="utf-8"?>
<set xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" android:ordering="sequentially">
    <objectAnimator android:interpolator="@android:anim/decelerate_interpolator" android:duration="250" android:valueFrom="0" android:valueTo="-32dp" android:valueType="floatType" android:propertyName="translationY"/>
    <objectAnimator android:interpolator="@android:anim/accelerate_interpolator" android:duration="250" android:valueFrom="-32dp" android:valueTo="0dp" android:valueType="floatType" android:propertyName="translationY"/>
    <set android:ordering="sequentially">
        <set android:ordering="together">
            <objectAnimator android:interpolator="@android:anim/decelerate_interpolator" android:duration="250" android:valueFrom="0" android:valueTo="16dp" android:valueType="floatType" android:propertyName="translationY"/>
            <objectAnimator android:interpolator="@android:anim/decelerate_interpolator" android:duration="250" android:valueFrom="1" android:valueTo="0.75" android:valueType="floatType" android:propertyName="scaleY"/>
        </set>
        <set android:ordering="together">
            <objectAnimator android:interpolator="@android:anim/accelerate_interpolator" android:duration="250" android:valueFrom="16dp" android:valueTo="0dp" android:valueType="floatType" android:propertyName="translationY"/>
            <objectAnimator android:interpolator="@android:anim/accelerate_interpolator" android:duration="250" android:valueFrom="0.75" android:valueTo="1" android:valueType="floatType" android:propertyName="scaleY"/>
        </set>
    </set>
</set>
